{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "strict": true,
    "strictPropertyInitialization": false,
    "baseUrl": "./",
    "paths": {
      "~/*": ["./src/*"],
      
    },
    "resolveJsonModule": true,
  }
}